package bean;

public class Student {
	private String hoTen;
	

	public String getHoTen() {
		return hoTen;
	}

	public void setHoTen(String hoTen) {
		this.hoTen = hoTen;
	}

	public void setMangToan(String mangToan) {
		System.out.println("Bạn "+hoTen+" hãy mang sách "+mangToan+" đi học");
	}
	public void setMangLy(String mangLy) {
		System.out.println("Bạn "+hoTen+" hãy mang sách "+mangLy+" đi học");
	}
	public void setMangSinh(String mangSinh) {
		System.out.println("Bạn "+hoTen+" hãy mang sách "+mangSinh+" đi học");
	}

}
