package student.bean;

public class Student {
	private String fullname,code,classStudent;
	private int old;
	private float lp1,lp2;
	public Student() {
		
	}
	public Student(String code,String fullname,String classStudent, int old, float lp1, float lp2) {
		this.fullname = fullname;
		this.code = code;
		this.classStudent = classStudent;
		this.old = old;
		this.lp1 = lp1;
		this.lp2 = lp2;
	}
	public String getFullname() {
		return fullname;
	}
	public void setFullname(String fullname) {
		this.fullname = fullname;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getClassStudent() {
		return classStudent;
	}
	public void setClassStudent(String classStudent) {
		this.classStudent = classStudent;
	}
	public int getOld() {
		return old;
	}
	public void setOld(int old) {
		this.old = old;
	}
	public float getLp1() {
		return lp1;
	}
	public void setLp1(float lp1) {
		this.lp1 = lp1;
	}
	public float getLp2() {
		return lp2;
	}
	public void setLp2(float lp2) {
		this.lp2 = lp2;
	}
}
