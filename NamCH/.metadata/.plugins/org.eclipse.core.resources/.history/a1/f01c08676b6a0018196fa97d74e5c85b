package student.bean;

public class Student {
	private String fullname,code,classStudent;
	private int old;
	private float lp1,lp2;
	public Student() {
		
	}
	public Student(String fullname,String code,String classStudent, int old, float lp1, float lp2) {
		this.fullname = fullname;
		this.code = code;
		this.classStudent = classStudent;
		this.old = old;
		this.lp1 = lp1;
		this.lp2 = lp2;
	}
}
